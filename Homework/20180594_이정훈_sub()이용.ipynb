    "import re\n",
    "def SquareNum(match) :\n",
    "    value = int(match.group())\n",
    "    return str(value ** 2)\n",
    "\n",
    "p = re.compile(r'\\d+')\n",
    "p.sub(SquareNum, 'Please, square the following numbers, 3 7 11 13 17 19')"
   ]
  }
